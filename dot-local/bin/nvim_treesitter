#!/usr/bin/env -S nlua
local treesitters = {
'awk',
'bash',
'comment',
'css',
'csv',
'diff',
'djot',
'dtd',
'ebnf',
'elixir',
'erlang',
'git_config',
'gitignore',
'gleam',
'gnuplot',
'html',
'http',
'javascript',
'jq',
'json',
'just',
'latex',
'ledger',
'make',
'markdown_inline',
'mermaid',
'nginx',
'printf',
'readline',
'regex',
'ssh_config',
'toml',
'xml',
'yaml'
}

vim.pack.add({
    {
    src = 'https://github.com/nvim-treesitter/nvim-treesitter',
    version = 'main'
    }})

local ok_ts, ts = pcall(require, 'nvim-treesitter')
if ok_ts then
ts.install(treesitters):wait(300000)
end 


